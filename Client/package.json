{
  "name": "client",
  "version": "1.0.0",
  "description": "Client-side Forte web application.",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --colors --progress --hot",
    "build:dev": "babel-node tools/build --development && npm run html",
    "build:prod": "babel-node tools/build --production && npm run html",
    "start:clean": "npm run build:dev && npm start",
    "clean": "npm run clean:modules && npm run clean:build && npm i",
    "clean:build": "rm ./build/bundle.* index.html",
    "clean:modules": "rm -r ./node_modules",
    "test": "cp -r ./build/bundle.js ./test/build/bundle.js && mocha --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "html": "cp ./src/index.html ./build",
    "less": "lessc ./src/bundle.less ./build/bundle.css"
  },
  "keywords": [],
  "repository": "https://github.com/srslafazan/f2.git",
  "author": "Shain Lafazan",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.0",
    "less-loader": "^2.2.3",
    "mocha-phantomjs": "^4.0.2",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "webpack": "^1.13.0"
  }
}
